<?xml version="1.0" encoding="utf-8"?>
<!--
ATTENTION: FOR UNITY3D
When you extend the strings.xml and you would like to use the values from the ARUnity6 plugin you
need to add the value to the UnityARPlayer-Project strings.xml.
That is because the AR6J gets build into a .jar file for Unit3D and the .jar files do not contain
resources.
The UnityARPlayer project is build as .aar and due to that it contains the resources.
-->
<resources>
    <string name="menuItem_settings">Settings</string>

    <string name="prefCategory_title_camera">Camera settings</string>
    <string name="pref_title_cameraResolution">Camera resolution</string>
    <string-array name="pref_entries_cameraResolution">
        <item>640x480   (4:3)</item>
    </string-array>
    <string-array name="pref_entryValues_cameraResolution">
        <item>640x480</item>
    </string-array>
    <string name="pref_defaultValue_cameraResolution">640x480</string>

    <string name="pref_title_cameraIndex">Change camera</string>
    <string name="settingsDesc">Open settings</string>
    <string name="pref_cameraResolutionString">pref_cameraResolution</string>
    <string name="pref_defaultValue_cameraIndex">0</string>
    <string-array name="pref_entries_cameraIndex">
        <item>Rear camera</item>
    </string-array>
    <string-array name="pref_entryValues_cameraIndex">
        <item>0</item>
    </string-array>

    <string-array name="menuItems">
        <item>@string/menuItem_settings</item>
    </string-array>

</resources>
